#include <stdio.h>
#include<stdlib.h>
int main (){
  	int m=0,i,b,j,n,l=0,k=1;
	  scanf("%d",&n);
	

   for(i=1;i<=n;i++){
	   for(j=0;j<3;j++){
	   	b=i;
	   	printf("%d",k+l);
	   	
	   	k=k*b;
	   	if(m%2!=0){
			l=1;
		   }else{
			   l=0;
		   }
		  if(j<2){
				 printf(" ");
		 }
	   }
	   l=0;
   	   m++;
   	   if(m%2!=0){
   	   	i--;
	  }
		k=k+1;
	   printf("\n");
	   k=i+1;

	}
   return 0;
}